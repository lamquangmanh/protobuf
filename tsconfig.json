{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {
      "@proto/*": ["proto/*"]
    }
  },
  "include": ["proto/**/**/*.proto"],
  "exclude": ["node_modules", "dist", "build"]
}
